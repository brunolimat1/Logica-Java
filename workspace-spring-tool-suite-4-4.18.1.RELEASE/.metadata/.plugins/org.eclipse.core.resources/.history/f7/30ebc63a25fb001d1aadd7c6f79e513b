package objetos;

public class Increment {

	public static void main(String[] args) {
		int c = 5;
		System.out.printf("c before postincrement: %d%n", c);
		System.out.printf("		 postincrementing: c: %d%n", c++);
		System.out.printf("c before postincrement: %d%n", c);
	
		System.out.println();
		
	}

}
